{
    "version": 2,
    "buildCommand": "next build",
    "deploymentFeedback": {
      "silent": false
    },
    "crons": [
      {
        "path": "/api/cron-maintenance-check",
        "schedule": "0 12 * * 1"
      }
    ],
    "regions": ["syd1"],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ]
  }