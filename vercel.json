{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      {
        "src": "/php-maintenance",
        "dest": "/php-maintenance.html"
      },
      {
        "src": "/api/maintenance-php.php",
        "dest": "/api/maintenance-php.php"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/maintenance-request",
        "destination": "/maintenance",
        "permanent": true
      }
    ]
  }